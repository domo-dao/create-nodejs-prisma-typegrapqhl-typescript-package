{
  "name": "insightt-BACKEND",
  "version": "1.0.0",
  "description": "Insightt Back-end Server",
  "main": "app.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "prisma:generate": "prisma generate",
    "start": "cross-env NODE_ENV=dev nodemon app.js",
    "test:start": "cross-env NODE_ENV=local nodemon server-email.js",
    "dev": "cross-env NODE_ENV=dev pm2 start app.js --name='insightt-api'",
    "local": "cross-env NODE_ENV=dev pm2-runtime start app.js --name='insightt-api' --watch",
    "staging": "cross-env NODE_ENV=staging pm2 start app.js --name='insightt-api'",
    "master": "cross-env NODE_ENV=master pm2 start app.js --name='insightt-api'",
    "api:start": "node app.js",
    "cron:start": "cross-env NODE_ENV=local nodemon server-cron.js",
    "cron:dev": "cross-env NODE_ENV=dev pm2 start server-cron.js --name='insightt-cron'",
    "cron:staging": "cross-env NODE_ENV=staging pm2 start server-cron.js --name='insightt-cron'",
    "cron:master": "cross-env NODE_ENV=master pm2 start server-cron.js --name='insightt-cron'",
    "worker:start": "node server-worker.js",
    "test": "cross-env NODE_ENV=dev jest",
    "docs": "apidoc -i server -o docs",
    "update:packages": "npm install -g npm-check-updates && ncu -u && npm install",
    "lint": "eslint ./server/ --ignore-path .gitignore",
    "format": "prettier --write \"./**/*.{js,json}\"",
    "sync:company:db": "node sync-company-db.js",
    "sync:company:db:dev": "cross-env NODE_ENV=dev node sync-company-db.js",
    "sync:company:db:stage": "cross-env NODE_ENV=staging node sync-company-db.js",
    "sync:company:db:master": "cross-env NODE_ENV=master node sync-company-db.js",
    "sequelize:companyDB:makeMigration": "sequelize-cli --options-path ./.sequelize-company-db --env company_db migration:generate --name",
    "sequelize:platformDB:makeMigration": "sequelize-cli --options-path ./.sequelize-platform-db --env platform_db migration:generate --name",
    "sequelize:platformDB:migrate": "sequelize --options-path ./.sequelize-platform-db --env platform_db db:migrate",
    "sequelize:platformDB:migrate:undo": "sequelize --options-path ./.sequelize-platform-db --env platform_db db:migrate:undo --name"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.55.0",
    "@hapi/joi": "^17.1.1",
    "@supercharge/promise-pool": "^2.3.2",
    "@types/graphql-fields": "^1.3.4",
    "apisauce": "^1.1.2",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "aws-sdk": "^2.739.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bull": "^3.22.9",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validation": "^3.0.5",
    "firebase-admin": "^9.5.0",
    "formidable": "^1.2.2",
    "google-distance-matrix": "^1.1.1",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.17.0",
    "html-minifier": "^4.0.0",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "lodash": "^4.17.20",
    "mjml": "^4.6.3",
    "moment": "^2.27.0",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-abort-controller": "^3.0.1",
    "node-cron": "^3.0.0",
    "node-geocoder": "^3.27.0",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pm2": "^5.1.2",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.4",
    "simple-json-db": "^2.0.0",
    "socket.io": "^4.4.0",
    "staticmaps": "^1.7.0",
    "stripe": "^8.222.0",
    "tslib": "^2.4.0",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.0",
    "winston": "^3.6.0",
    "winston-cloudwatch": "^4.0.1",
    "winston-daily-rotate-file": "^4.5.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@prisma/client": "~4.4.0",
    "apidoc": "^0.25.0",
    "eslint": "^7.13.0",
    "husky": "^4.3.0",
    "jest": "^27.4.7",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.0",
    "prisma": "~4.4.0",
    "sequelize-cli": "^6.2.0",
    "typegraphql-prisma": "^0.22.0"
  },
  "keywords": [
    "node.js",
    "express.js",
    "passport",
    "jsonwebtoken",
    "sequelize",
    "mysql2",
    "winston",
    "lodash",
    "moment",
    "aws-sdk",
    "prettier"
  ],
  "engines": {
    "node": "<17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --write"
    ]
  }
}
